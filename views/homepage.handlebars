<div class="col-12">
  <div class="row">
    <div class="col-12">MechDog TechBlogs</div>
    {{#each blogs as |blog|}}
    <div class="col-12 bg-dark text-light"><a href="/blogs/{{blog.id}}">{{blog.title}}</a></div>
    <div class="col-6">{{blog.user.username}}</div>
    <div class="col-6">{{format_date blog.createdAt}}</div>
    {{/each}}
  </div>
</div>